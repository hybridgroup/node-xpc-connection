{
  "name": "xpc-connection",
  "version": "0.0.4",
  "description": "XPC Connection binding for node.js",
  "main": "index.js",
  "scripts": {
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandeepmistry/node-xpc-connection.git"
  },
  "bugs": {
    "url": "https://github.com/sandeepmistry/node-xpc-connection/issues"
  },
  "keywords": [
    "XPC",
    "XPC connection"
  ],
  "author": "Sandeep Misty <sandeep.mistry@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.8"
  },
  "os": [ "darwin" ],
  "dependencies"  : {
      "node-pre-gyp": "0.5.x"
    },
  "bundledDependencies":["node-pre-gyp"],
  "devDependencies": {
    "aws-sdk": "~2.0.0-rc.15"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "binding",
    "module_path" : "./build/{name}/v{version}/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path" : "./{name}/v{version}/{configuration}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host"        : "https://node-xpc-connection.s3.amazonaws.com"
  }
}
